#send-mess-popup.mh-popup.container.cnt--ind-15.send-mess-popup.mfp-hide
    .popup-title
        h2.title-6 Send Message
        p.pt__text You can use my contact form, i’m love to talk.
    form(action="")
        .smp__field
            .row
                .col-exs-full.col-sm-half
                    input.smp__input(
                        type="text",
                        placeholder="Name*"
                    )
                .col-exs-full.col-sm-half
                    input.smp__input(
                        type="email",
                        placeholder="Email*"
                    )
        .smp__field
            input.smp__input.smp__input--italic(
                type="text",
                placeholder="Subject"
            )
        .smp__field
            textarea.smp__input(
                placeholder="Message*"
            )
        .smp__foot
            button.smp__btn Send Message
                img.svg-icon(
                    src="assets/img/svg/message.svg"
                    width="16"
                    height="16"
                )

